{
  "name": "travel-rule-api-docs",
  "version": "1.0.0",
  "description": "Travel Rule API Documentation",
  "main": "index.html",
  "scripts": {
    "start": "http-server -p 8080 -o",
    "build": "redoc-cli build openapi.yaml --output index.html --title 'Travel Rule API Documentation'",
    "serve": "redoc-cli serve openapi.yaml --port 8080",
    "validate": "swagger-codegen validate -i openapi.yaml"
  },
  "keywords": [
    "travel-rule",
    "api",
    "documentation",
    "openapi",
    "swagger"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "redoc-cli": "^0.13.21",
    "swagger-codegen": "^2.4.30"
  }
}
